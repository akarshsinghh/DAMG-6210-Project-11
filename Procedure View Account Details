CREATE OR REPLACE PROCEDURE Account_details(custid int) AS
  c1 SYS_REFCURSOR;  
BEGIN

  open c1 for
  SELECT ACCOUNT.ACCOUNTNUMBER, CUSTOMER.FIRST_NAME, CUSTOMER.LAST_NAME,ACCOUNT.ACCOUNT_TYPE, ACCOUNT.BRANCHIFSC, ACCOUNT.ACCOUNT_BALANCE FROM CUSTOMER
INNER JOIN ACCOUNT ON CUSTOMER.CUSTOMER_ID= ACCOUNT.CUSTOMER_ID
 WHERE CUSTOMER.CUSTOMER_ID= custid;

  DBMS_SQL.RETURN_RESULT(c1);
END;

--TESTING

EXEC Account_details(100101);
